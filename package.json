{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "web-search",
  "title": "Web Search",
  "description": "Extension to search the sites and see the results in Raycast",
  "icon": "command-icon.png",
  "author": "Maksim_Zemlyanikin",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search With Google",
      "description": "Command to search using the Google engine",
      "icon": "command-icon.png",
      "mode": "view"
    }
  ],
  "categories": [
    "Web"
  ],
  "dependencies": {
    "@raycast/api": "^1.48.5",
    "googlethis": "^1.7.1",
    "ts-debounce": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}